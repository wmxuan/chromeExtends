{
    "manifest_version":2,
    "name":"天气预报",
    "version":"1.0.0",
    "description":"查看未来两周的天气预报",
    "icons":{
        "16":"./img/me.png",
        "48":"./img/me.png",
        "128":"./img/me.png"
    },
    "browser_action":{
        "default_icon":{
            "19":"./img/me.png",
            "38":"./img/me.png"
        },
        "default_title":"天气预报",
        "default_popup":"popup.html"
    },
    "options_page":"options.html",
    "permissions":[
        "http://api.openweathermap.org/data/2.5/forecast?q=*"
    ]
}